{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "indent": "off",
        "linebreak-style": "off",
        "space-before-blocks": "off",
        "require-jsdoc": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}